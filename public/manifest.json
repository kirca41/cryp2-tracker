{
  "manifest_version": 2,
  "name": "Cryp2 Tracker",
  "author": "Kiril Kovacheski",
  "version": "0.1.0",
  "description": "Track your favourite cryptocurrencies",
  "icons": {
    "16": "cryp2Fav16.png",
    "32": "cryp2Fav32.png",
    "192": "cryp2Fav192.png"
  },
  "permissions": [ "storage" ],
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Display cryptocurrencies"
  }
}